{
	"compilerOptions": {
		"target": "ES2017",
		"lib": ["ES2019", "DOM"],
		"jsx": "react",
		"module": "commonjs",
		"resolveJsonModule": true,
		"outDir": "dist",
		"typeRoots": ["./src/types/"],
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	},
	"include": ["./src/**/*.*"]
}
